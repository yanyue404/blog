{
  "name": "issue-blog",
  "version": "0.0.1",
  "description": "github issue blog",
  "productName": "闫越的网络日志",
  "cordovaId": "org.cordova.quasar.app",
  "author": "ttop5 <ttop5@qq.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "dev:ssr": "quasar dev -m ssr",
    "build": "quasar build",
    "build:ssr": "quasar build --mode ssr",
    "deploy": "quasar build && node publish.js",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --fix --ext .js --ext .jsx --ext .vue src/",
    "lint:html": "eslint **/*.js -f html -o ./reports/lint-results.html"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.18.1",
    "buffer": "^6.0.3",
    "github-markdown-css": "^3.0.1",
    "quasar": "^1.0.0",
    "timeago.js": "^4.0.0-beta.2",
    "vue-i18n": "^8.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@quasar/app": "^1.0.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "gh-pages": "^2.0.1",
    "husky": "^3.0.1"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
